<h1 class="title"> Workout Log</h1>  
<table> 
    <% workouts.forEach(function(workout){  %>
        <tr class="divRow"> 
                <td><a href="/workout/allWorkouts/<%=workout.id%>"><button class="divButton"><%=(workout.date).toDateString() %> 
                <%=workout.targetMuscleGroups1 %></td>  
           </button> </a>
            <td class ="editButton">E</td>
            <td><button type="submit" class ="deleteButton"><a href="<%= `workouts/${workout.id}/delete?_method=GET` %>"> X </a> </button>
            </td>
        </tr> 

   <% }) %>
</table>
<br>
<a href="/workout/addWorkout"><button type="submit" class="redButton">Add New Entry</button></a>

